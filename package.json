{
  "name": "@naturalcycles/js-lib",
  "version": "14.55.0",
  "scripts": {
    "prepare": "husky",
    "build": "dev-lib build-esm-cjs",
    "test": "TZ=UTC vitest",
    "lint": "dev-lib lint",
    "bt": "dev-lib bt",
    "lbt": "dev-lib lbt",
    "test-tz1": "TZ=Europe/Stockholm yarn test local",
    "test-tz2": "TZ=JST-9 yarn test local",
    "test-ny": "TZ=GMT-0500 yarn test localTime",
    "docs-dev": "vitepress dev docs --open",
    "docs-build": "vitepress build docs",
    "docs-preview": "vitepress preview docs"
  },
  "dependencies": {
    "tslib": "^2",
    "zod": "^3"
  },
  "devDependencies": {
    "@naturalcycles/bench-lib": "^3",
    "@naturalcycles/dev-lib": "^15",
    "@naturalcycles/nodejs-lib": "^13",
    "@naturalcycles/time-lib": "^3",
    "@types/crypto-js": "^4",
    "@types/node": "^22",
    "@types/semver": "^7",
    "crypto-js": "^4",
    "prettier": "^3",
    "vitepress": "^1",
    "vitest": "^3",
    "vue": "^3"
  },
  "files": [
    "dist",
    "dist-esm",
    "cfg",
    "src",
    "!src/test",
    "!src/**/*.test.*",
    "!src/**/__snapshots__"
  ],
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=20.13.0"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NaturalCycles/js-lib"
  },
  "description": "Standard library for universal (browser + Node.js) javascript",
  "keywords": [
    "js-lib",
    "lodash",
    "p-map",
    "standard library"
