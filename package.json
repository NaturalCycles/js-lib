{
  "name": "@naturalcycles/monorepo",
  "type": "module",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "check": "pnpm build && pnpm lint && pnpm test",
    "lint": "pnpm --filter '@naturalcycles/*' run lint",
    "build": "pnpm --filter '@naturalcycles/*' run build",
    "test": "pnpm --filter '@naturalcycles/*' run test",
    "js-lib:build": "pnpm --filter @naturalcycles/js-lib run build",
    "js-lib:test": "pnpm --filter @naturalcycles/js-lib run test",
    "__build": "turbo run build",
    "__dev": "turbo run dev",
    "__lint": "turbo run lint",
    "__check-types": "turbo run check-types",
    "docs-dev": "vitepress dev docs --open",
    "docs-build": "vitepress build docs",
    "docs-preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@naturalcycles/bench-lib": "^4.0.1",
    "@naturalcycles/dev-lib": "workspace:*",
    "@naturalcycles/nodejs-lib": "workspace:*",
    "@types/node": "^22.15.3",
    "tsx": "^4.19.4",
    "vitepress": "^1.6.3",
    "vitest": "^3.1.2",
    "vue": "^3.5.13"
  },
  "engines": {
    "node": ">=22.12.0",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.10.0"
}
